{
    "name": "budgetcontrol/workspace",
    "description": "defines the dependencies required for a PHP microservice managing workspaces in a Budget application",
    "type": "none",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mlabfactory/Libs_Mailer.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BudgetControl/Libs_BcConnector.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/MaliRobot/aws-cognito.git"
        },
        {
            "type":"vcs",
            "url":"git@github.com:BudgetControl/MailerService.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mlabfactory/phpunit-self-api-integration.git"
        },
        {
            "type":"vcs",
            "url":"https://github.com/BudgetControl/Library.git"
        },
        {
            "type":"vcs",
            "url":"git@github.com:BudgetControl/SeedsTestLibs.git"
        },
        {
            "type":"vcs",
            "url":"git@github.com:BudgetControl/Schema-Registry-Service.git"
        },
        {
            "type":"vcs",
            "url":"git@github.com:BudgetControl/LibsCryptable.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "slim/slim": "^4.13",
        "doctrine/dbal": "^4.0",
        "symfony/var-dumper": "^7.0",
        "ramsey/uuid": "^4.7",
        "monolog/monolog": "^3.5",
        "vlucas/phpdotenv": "^5.6",
        "illuminate/database": "^11.0",
        "laravel/serializable-closure": "^1.3",
        "symfony/serializer": "^7.0",
        "robmorgan/phinx": "^0.16.0",
        "slim/http": "^1.3",
        "slim/psr7": "^1.6",
        "psr/log": "^3.0",
        "illuminate/events": "^11.0",
        "symfony/property-access": "^7.0",
        "illuminate/filesystem": "^11.0",
        "logtail/monolog-logtail": "^3.1",
        "budgetcontrol/mailer": "^v0.2",
        "budgetcontrol/library": "^v1.1",
        "budgetcontrol/schema-registry-service": "v1.0-beta",
        "budgetcontrol/cryptable": "^v0.1"
    },
    "require-dev": {
        "nikic/php-parser": "^5.0",
        "phpunit/phpunit": "^11.0",
        "fakerphp/faker": "^1.23",
        "budgetcontrol/seeds": "^v0.1",
        "mlabfactory/phpunit-self-api-integration": "^v1.1-beta"
    },
    "autoload": {
        "psr-4": {
            "Budgetcontrol\\Workspace\\": "src/"
        },
        "files": [
            "config/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Marco De Felice",
            "email": "marco.defelice@mlabfactory.it"
        }
    ],
    "minimum-stability": "stable"
}
